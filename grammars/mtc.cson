
'scopeName': 'source.mtc'
'name': 'MTC'
'fileTypes': ['mtc',]

'patterns': [
    {
	    'match': '\\b(([0-9]+\\.?[0-9]*)|(\\.[0-9]+))\\b'
	    'name': 'constant.numeric.mtc'
    },
    {
        'match': '\\b(\\w+)(=|:)'
        'captures':
            '1':
                'name': 'entity.name.function.mtc'
            '2':
                'name': 'keyword.control.mtc'
        'name': 'meta.function.mylanguage'
    }
]


